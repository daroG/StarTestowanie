<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <Pane prefHeight="200.0" prefWidth="200.0">
      <children>
         <TableView fx:id="table" minHeight="100.0" prefHeight="180.0" prefWidth="1000.0">
           <columns>
             <TableColumn prefWidth="75.0" text="C1" />
             <TableColumn prefWidth="75.0" text="C2" />
             <TableColumn prefWidth="75.0" text="C2" />
             <TableColumn prefWidth="75.0" text="C2" />
             <TableColumn prefWidth="75.0" text="C2" />
           </columns>
         </TableView>
          <Button fx:id="removeStar" layoutX="409.0" layoutY="180.0" onAction="#removeStar" text="Usuń zaznaczony element">
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </Button>
      </children>
      <VBox.margin>
         <Insets bottom="30.0" />
      </VBox.margin>
   </Pane>
   <Pane nodeOrientation="LEFT_TO_RIGHT" prefHeight="251.0" prefWidth="597.0">
      <VBox.margin>
         <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
      </VBox.margin>
      <children>
         <TextField fx:id="name" layoutX="208.0" layoutY="56.0" promptText="Nazwa: ABC1234" />
         <TextField fx:id="constellation" layoutX="395.0" layoutY="56.0" promptText="Gwiazdozbiór" />
         <TextField fx:id="declination" layoutX="583.0" layoutY="56.0" promptText="Deklinacja" />
         <TextField fx:id="rightAscension" layoutX="208.0" layoutY="115.0" promptText="Rektascencja" />
         <TextField fx:id="distance" layoutX="395.0" layoutY="115.0" promptText="Odległość" />
         <TextField fx:id="observedMagnitude" layoutX="583.0" layoutY="115.0" promptText="Obs. wielkość gwiadowa" />
         <TextField fx:id="temperature" layoutX="208.0" layoutY="170.0" promptText="Temperatura" />
         <TextField fx:id="mass" layoutX="395.0" layoutY="170.0" promptText="Masa" />
         <Button fx:id="addStar" layoutX="583.0" layoutY="170.0" mnemonicParsing="false" onAction="#addStar" prefHeight="25.0" prefWidth="149.0" text="Dodaj Gwiazdę">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Label fx:id="label" layoutX="456.0" layoutY="220.0" text="Label" />
         <Label fx:id="nameSub" layoutX="268.0" layoutY="89.0" text="Label" visible="false" />
         <Label fx:id="constellationSub" layoutX="455.0" layoutY="89.0" text="Label" visible="false" />
         <Label fx:id="declinationSub" layoutX="644.0" layoutY="89.0" text="Label" visible="false" />
         <Label fx:id="rightAscensionSub" layoutX="268.0" layoutY="147.0" text="Label" visible="false" />
         <Label fx:id="distanceSub" layoutX="456.0" layoutY="147.0" text="Label" visible="false" />
         <Label fx:id="observedMagnitudeSub" layoutX="643.0" layoutY="147.0" text="Label" visible="false" />
         <Label fx:id="temperatureSub" layoutX="268.0" layoutY="203.0" text="Label" visible="false" />
         <Label fx:id="massSub" layoutX="455.0" layoutY="203.0" text="Label" visible="false" />
      </children>
   </Pane>
</VBox>
